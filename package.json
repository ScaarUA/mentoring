{
  "name": "mentoring-project-huseinov",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "start:react": "cross-env framework=react npm run start",
    "start:react-bh": "cross-env framework=react-bh npm run start",
    "start": "concurrently \"npm run dev\" \"npm run serve\"",
    "serve": "nodemon server",
    "build": "webpack",
    "cordova:emulate": "npm run build && ncp public cordova/www && cd cordova && cordova emulate browser",
    "build:android": "npm run build && ncp public cordova/www && cd cordova && cordova build android && copy:apk",
    "prepare:android": "cd cordova && cordova platform add android",
    "copy:apk": "ncp cordova/platforms/android/build/outputs/apk/android-debug.apk cordova/mentoring.apk",
    "dev": "webpack-dev-server --debug --open",
    "test": "webpack --config ./config/webpack/webpack.config.test.js && mocha test/test.bundle.js --color --ansi",
    "tdd": "node ./config/env/tdd.js",
    "eslint": "eslint config/**/*.js server/**/*.js",
    "eslint-changed": "node ./config/env/lintModified.js --type=eslint",
    "tslint": "tslint src/**/*.ts",
    "tslint-changed": "node ./config/env/lintModified.js --type=tslint",
    "validate": "npm ls",
    "debug": "devtool server/index.js --watch"
  },
  "author": "Bohdan Huseinov bohdan_huseinov@epam.com",
  "license": "ISC",
